{
  "extends": [
    "xo-space/esnext",
    "xo/esnext",
		"xo-react",
    "plugin:react/recommended",
    "prettier",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true,
    "jest": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "react/no-unescaped-entities": 0
  }
}
